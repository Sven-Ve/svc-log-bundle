{% trans_default_domain 'LogBundle' %}
{% extends 'base.html.twig' %}

{% block title %}
  {% trans %}Daily summary{% endtrans %}
{% endblock %}

{% block body %}
<style>
table, th, td {
  border: 1px solid black;
  padding: 3px;
  border-collapse: collapse;
}
</style>

  <h1>
    <i class="fa-regular fa-rectangle-list"></i>
    {% trans %}Daily summary{% endtrans %}
  </h1>

  {% for daily_aggr in daily_aggrs | default(null) %}
    {{ include("@SvcLog/daily_summary/_daily_aggr.html.twig", {daily_aggr: daily_aggr}) }}
  {% endfor %}

  {% for daily_list in daily_lists | default(null) %}
    {{ include("@SvcLog/daily_summary/_daily_list.html.twig", {daily_list: daily_list}) }}
  {% endfor %}

{% endblock %}
